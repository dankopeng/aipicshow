{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "wish100",
	"compatibility_date": "2025-04-01",
	"main": "./server.ts",
	"assets": {
		"directory": "./build/client"
	},
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true,
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "wish100-db",
			"database_id": "39c2bbeb-96bc-4b44-be4b-c5dd45b1fdef"
		}
	],
	"r2_buckets": [
		{
			"binding": "IMAGES",
			"bucket_name": "wish100-images"
		}
	]
}
